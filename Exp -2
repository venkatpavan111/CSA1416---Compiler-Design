```c
#include <stdio.h>
#include <ctype.h>
#include <string.h>

void analyze(char *code) {
    while (*code) {
        if (isspace(*code)) {
            code++;
            continue;
        }
        if (strncmp(code, "//", 2) == 0) {
            printf("Single-line Comment\n");
            return;
        }
        if (strncmp(code, "/*", 2) == 0) {
            printf("Multi-line Comment\n");
            return;
        }
        if (isalpha(*code) || *code == '_') {
            printf("Identifier: %c\n", *code);
        } else if (isdigit(*code)) {
            printf("Constant: %c\n", *code);
        } else {<img width="570" height="222" alt="comment or not" src="https://github.com/user-attachments/assets/611c72b0-9c68-4df3-b3ab-804ad54eb467" />

            printf("Operator/Symbol: %c\n", *code);
        }
        code++;
    }
}

int main() {
    char code1[] = "// This is a comment";
    char code2[] = "/* This is a multi-line comment */";
    char code3[] = "int x = 10;";
    
    analyze(code1);
    analyze(code2);
    analyze(code3);
    
    return 0;
}
```


